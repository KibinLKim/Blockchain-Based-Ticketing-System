        <input type="hidden" name="current_price" value="<%=ticketinfo.ticket_price%>">
        <input type="hidden" name="ticket_id" value="<%=ticketinfo.ticket_id%>">
        <input type="datetime-local" name="starting_time"> Starting Time <br>
        <input type="datetime-local" name="end_time"> End Time <br>
        <input type="number" name="starting_price"> starting_price <br>
    </form>
</body>
